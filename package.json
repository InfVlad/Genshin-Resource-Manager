{
  "name": "genshin-resource-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "scrape": "tsx ./src/scraper/main.ts && pnpm prettier src/data --write",
    "prepare": "husky install",
    "prisma:generate": "dotenv -e .env.local -- pnpm exec prisma generate && pnpm eslint prisma/zod --fix",
    "prisma:inital:migration": "dotenv -e .env.local -- pnpm exec prisma migrate dev --name init",
    "prisma:migrate": "dotenv -e .env.local -- pnpm exec prisma migrate dev",
    "prisma:dev:deploy": "dotenv -e .env.local -- pnpm exec prisma migrate deploy",
    "prisma:studio": "dotenv -e .env.local -- pnpm exec prisma studio",
    "db:start": "docker compose --env-file ./.env.local up",
    "db:start:test": "docker compose --env-file ./.env.test up",
    "prisma:migrate:test": "dotenv -e .env.test -- pnpm exec prisma migrate dev && pnpm eslint prisma/zod --fix",
    "prisma:seed:test": "dotenv -e .env.test -- tsx ./prisma/seed",
    "prisma:studio:test": "dotenv -e .env.test -- pnpm exec prisma studio",
    "test": "dotenv -e .env.test vitest"
  },
  "dependencies": {
    "@auth/core": "^0.16.1",
    "@auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^5.4.2",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.40.0",
    "@trpc/next": "^10.40.0",
    "@trpc/react-query": "^10.40.0",
    "@trpc/server": "^10.40.0",
    "axios": "^1.5.1",
    "cheerio": "1.0.0-rc.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "next": "13.5.4",
    "next-auth": "^4.23.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "^2.0.0",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@types/node": "20.8.5",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "autoprefixer": "10.4.16",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.51.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^5.4.2",
    "tailwindcss": "3.3.3",
    "tsx": "^3.13.0",
    "typescript": "5.2.2",
    "vitest": "^0.34.6",
    "zod-prisma": "^0.5.4"
  }
}
